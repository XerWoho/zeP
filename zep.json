{
  "author": "Xerrs",
  "tags": [],
  "zig_version": "0.15.2",
  "repo": "https://github.com/XerWoho/zeP",
  "name": "zeP",
  "cmd": [
    {
      "name": "release_windows",
      "cmd": "zig build -Doptimize=ReleaseFast -freference-trace -Dtarget=x86_64-windows-msvc -p tempR/w"
    },
    {
      "name": "release_linux",
      "cmd": "zig build -Doptimize=ReleaseFast -freference-trace -Dtarget=x86_64-linux -p tempR/l"
    }
  ],
  "description": "A new zig Package manager, and version manager for zig.",
  "version": "0.9",
  "license": "GPLv3",
  "packages": [
    "clap@0.11.0",
    "logly@0.0.8",
    "bufzilla@0.3.0",
    "zeit@0.7.0"
  ],
  "dev_packages": [],
  "build": {
    "entry": "/src/main.zig",
    "target": "x86_64-windows"
  }
}